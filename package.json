{
  "name": "xdm",
  "version": "1.7.0",
  "description": "an MDX compiler",
  "license": "MIT",
  "keywords": [
    "xdm",
    "mdx",
    "markdown",
    "jsx"
  ],
  "repository": "wooorm/xdm",
  "bugs": "https://github.com/wooorm/xdm/issues",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/wooorm"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "type": "module",
  "types": "./index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/",
    "esbuild.js",
    "esbuild.d.ts",
    "esm-loader.js",
    "index.js",
    "index.d.ts",
    "register.cjs",
    "rollup.js",
    "rollup.d.ts",
    "webpack.cjs"
  ],
  "dependencies": {
    "@rollup/pluginutils": "^4.0.0",
    "@types/loader-utils": "^2.0.0",
    "astring": "^1.6.0",
    "deasync": "^0.1.0",
    "estree-util-build-jsx": "^2.0.0",
    "estree-util-is-identifier-name": "^2.0.0",
    "estree-walker": "^3.0.0",
    "hast-util-to-estree": "^1.2.0",
    "loader-utils": "^2.0.0",
    "markdown-extensions": "^1.0.0",
    "mdast-util-mdx": "^0.1.0",
    "micromark-extension-mdxjs": "^0.3.0",
    "periscopic": "^3.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "stringify-entities": "^4.0.0",
    "unified": "^9.0.0",
    "unist-builder": "^2.0.0",
    "unist-util-position-from-estree": "^1.0.0",
    "unist-util-stringify-position": "^2.0.0",
    "unist-util-visit": "^2.0.0",
    "vfile": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@emotion/react": "^11.0.0",
    "@mdx-js/react": "2.0.0-next.8",
    "@theme-ui/preset-base": "^0.6.0",
    "@types/babel__core": "^7.0.0",
    "@types/node": "^14.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/tape": "^4.0.0",
    "@types/webpack": "^4.0.0",
    "@vue/babel-plugin-jsx": "^1.0.0",
    "@vue/server-renderer": "^3.0.0",
    "c8": "^7.0.0",
    "esbuild": "^0.11.0",
    "eslint-plugin-es": "^4.0.0",
    "eslint-plugin-security": "^1.0.0",
    "estree-to-babel": "^3.0.0",
    "nanoid": "^3.0.0",
    "preact": "^10.0.0",
    "preact-render-to-string": "^5.0.0",
    "prettier": "^2.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rehype-katex": "^4.0.0",
    "rehype-raw": "^5.1.0",
    "remark-cli": "^9.0.0",
    "remark-footnotes": "^3.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-math": "^4.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.0.0",
    "source-map": "^0.7.0",
    "tape": "^5.0.0",
    "theme-ui": "^0.6.0",
    "type-coverage": "^2.0.0",
    "typescript": "^4.0.0",
    "vue": "^3.0.0",
    "webpack": "^5.0.0",
    "xo": "^0.38.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --no-warnings --experimental-loader=./esm-loader.js test/index.js && node test/register.cjs",
    "test-coverage": "c8 --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 --reporter lcov node --no-warnings --experimental-loader=./esm-loader.js test/index.js",
    "prebuild": "rimraf \"*.d.ts\" \"{lib,test}/**/*.d.ts\"",
    "build": "tsc && type-coverage",
    "test": "npm run format && npm run test-coverage && npm run build",
    "prepublishOnly": "npm run build"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "envs": [
      "shared-node-browser"
    ],
    "extensions": [
      "cjs"
    ],
    "prettier": true,
    "rules": {
      "complexity": "off",
      "import/no-mutable-exports": "off",
      "max-depth": "off",
      "new-cap": "off",
      "no-var": "off",
      "prefer-arrow-callback": "off",
      "prefer-const": "off"
    },
    "overrides": [
      {
        "files": [
          "lib/**/*.js"
        ],
        "extends": [
          "plugin:es/restrict-to-es2019",
          "plugin:security/recommended"
        ],
        "rules": {
          "security/detect-object-injection": "off"
        }
      },
      {
        "files": [
          "lib/run.js"
        ],
        "rules": {
          "no-new-func": "off"
        }
      },
      {
        "files": [
          "test/**/*.js"
        ],
        "rules": {
          "import/no-unassigned-import": "off"
        }
      },
      {
        "files": [
          "lib/integration/esbuild.js"
        ],
        "rules": {
          "eqeqeq": [
            "error",
            "always",
            {
              "null": "ignore"
            }
          ],
          "no-eq-null": "off",
          "security/detect-non-literal-fs-filename": "off"
        }
      }
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      [
        "lint-no-html",
        false
      ]
    ]
  },
  "typeCoverage": {
    "atLeast": 80,
    "detail": true,
    "strict": true,
    "ignoreCatch": true,
    "ignoreFiles": [
      "lib/util/resolve-evaluate-options.{d.ts,js}"
    ]
  }
}
